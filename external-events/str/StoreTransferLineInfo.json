{
    "id": "hiiretail.contracts.logistics.stc.StoreTransferLineInfo",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "StoreTransferLineInfo",
    "description": "Represents a line for an item being transferred",
    "unevaluatedProperties": false,
    "allOf": [
        {
            "type": "object",
            "required": [
                "lineInfoId",
                "quantity"
            ],
            "properties": {
                "lineInfoId": {
                    "type": "string",
                    "description": "Identifier of the line info"
                },
                "quantity": {
                    "type": "number",
                    "description": "Quantity of the item being transferred in this line info"
                },
                "batchNumber": {
                    "type": ["string", "null"],
                    "description": "Batch number of the item being transferred in this line info"
                },
                "serialNumber": {
                    "type": ["string", "null"],
                    "description": "Serial number of the item being transferred in this line info"
                },
                "bestBeforeDate": {
                    "type": ["null", "string"],
                    "format": "date",
                    "description": "Date to indicate the best before date"
                },
                "expirationDate": {
                    "type": ["null", "string"],
                    "format": "date",
                    "description": "Date to indicate the expiration date"
                }
            }
        }
    ]
}
