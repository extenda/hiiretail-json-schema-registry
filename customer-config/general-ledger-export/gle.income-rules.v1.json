{
  "$id": "gle.income-rules.v1.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Income Accounting Rules",
  "description": "List of Income rules for general ledger accounting and export",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string"
      },
      "config": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "enum": [
              "REASON_CODE",
              "DEFAULT"
            ]
          },
          "accountId": {
            "type": "string"
          },
          "value": {
            "type": "string"
          },
          "taxAccountId": {
            "type": "string"
          },
          "taxRate": {
            "type": "number"
          }
        },
        "required": [
          "type",
          "accountId"
        ]
      }
    },
    "required": [
      "id",
      "config"
    ]
  }
}
