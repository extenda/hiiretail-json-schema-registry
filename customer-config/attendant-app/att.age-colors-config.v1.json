{
  "$id": "att.age-colors-config.v1",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Age Range Color Configuration",
  "type": "array",
  "description": "List of all color configurations based on age range",
  "items": {
    "type": "object",
    "properties": {
      "range": {
        "type": "array",
        "minItems": 2,
        "maxItems": 2,
        "items": {
          "type": "number"
        }
      },
      "color": {
        "type": "string"
      }
    },
    "required": ["range", "color"],
    "additionalProperties": false
  }
}
