{
  "$id": "att.boolean-config.v1",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Boolean Config Schema",
  "type": "object",
  "properties": {
    "^[a-zA-Z0-9_-]+$": {
      "type": "object",
      "properties": {
        "value": { "type": "boolean" },
        "description": { "type": "string" }
      },
      "required": ["value"],
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
